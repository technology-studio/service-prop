{
  "compilerOptions": {
    "lib": ["esnext"],
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noEmit": false,
    "declaration": true,
    "esModuleInterop": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "sourceMap": true
  }
}
